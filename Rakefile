# -*- ruby -*-

require 'rubygems'
require 'hoe'
require './lib/gemfirecache_store.rb'

Hoe.new('activesupport-gemfirecache', ActiveSupport::Cache::GemfireCacheStore::VERSION) do |p|
  p.developer('Adam Denenberg', 'adam@dberg.org')
  p.url = 'http://github.com/denen99/activesupport-gemfirecache'
  p.blog_categories = ['activesupport-gemfirecache', 'activesupport', 'gemfire', 'cache', 'jruby', 'rails', 'ruby', 'java']
end

